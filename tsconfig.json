{
	"compilerOptions": {
		"outDir": "./dist/",
		"baseUrl": ".",
		"paths": {
			"*": [
				"*",
				"src/*"
			]
		},
		"sourceMap": true,
		"noImplicitAny": true,
		"module": "commonjs",
		"target": "es5",
		"experimentalDecorators": true,
		"allowUnreachableCode": true,
		"jsx": "react",
		"allowJs": true,
		"typeRoots": [
			"./node_modules/@types"
		]
	}
}